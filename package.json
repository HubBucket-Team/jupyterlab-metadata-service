{
  "name": "jupyterlab-metadata-service",
  "version": "0.1.0",
  "description": "A JupyterLab extension for Metadata Service",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/jupyterlab/jupyterlab-metadata-service",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab-metadata-service/issues"
  },
  "license": "BSD-3-Clause",
  "author": "CalPoly-Quansight",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab-metadata-service.git"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "^1.0.0",
    "@jupyterlab/codeeditor": "^1.0.0",
    "@jupyterlab/completer": "^1.0.0",
    "@jupyterlab/coreutils": "^3.0.0",
    "@jupyterlab/docregistry": "^1.0.0",
    "@jupyterlab/launcher": "^1.0.0",
    "@jupyterlab/mainmenu": "^1.0.0",
    "@phosphor/coreutils": "^1.3.0",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.6.0",
    "react": "~16.4.2",
    "react-dom": "~16.4.2"
  },
  "devDependencies": {
    "typescript": "~3.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  }
}
